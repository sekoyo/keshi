{
  "name": "keshi",
  "version": "3.0.0",
  "description": "An in-memory cache for Node and the browser designed for Promises",
  "repository": "https://github.com/DominicTobias/keshi",
  "keywords": [
    "node",
    "browser",
    "in-memory",
    "cache",
    "store",
    "async",
    "promises",
    "fetch"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepublishOnly": "tsc && vite build"
  },
  "author": "Dominic Tobias (https://github.com/DominicTobias/)",
  "license": "ISC",
  "dependencies": {
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/ms": "^0.7.34",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.1.8"
  }
}
